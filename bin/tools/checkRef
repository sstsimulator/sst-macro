#! /usr/bin/env python

import os
import ref
import re

def getTime(fname):
  text = open(fname).read()
  match = re.compile("timated total runtime of\s+(\d+[.]\d+)").search(text)
  if match:
    return match.groups()[0]
  else:
    return "No time"

def check(tmpFile, refFile):
  print fname
  print getTime(tmpFile)
  print getTime(refFile)
  print "\n"


ref.check_files(check)


